{
    "apps": [
        {
            "name": "wb_monit",
            "script": "./app.js",
             "exec_mode": "fork",
            "instances": 1,
            "autorestart": true,
            "restart_delay": 5000,
            "max_restarts": 10,
            "watch": false,
            "max_memory_restart": "1G",
            "error_file": "./logs/err.log",
            "out_file": "./logs/out.log",
            "log_file": "./logs/combined.log",
            "time": true
        }
    ]
}
